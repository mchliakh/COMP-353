<%- model_class = MissionSheet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:mission_id) %>:</strong></dt>
  <dd><%= @mission_sheet.mission_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:actual_start_time) %>:</strong></dt>
  <dd><%= @mission_sheet.actual_start_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:actual_end_time) %>:</strong></dt>
  <dd><%= @mission_sheet.actual_end_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_odometer) %>:</strong></dt>
  <dd><%= @mission_sheet.start_odometer %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end_odometer) %>:</strong></dt>
  <dd><%= @mission_sheet.end_odometer %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mission_sheets_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mission_sheet_path(@mission_sheet), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mission_sheet_path(@mission_sheet),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
